{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
      "no-console": "off",
      "vue/no-use-v-if-with-v-for": "off",
      "vue/valid-v-for": "off",
      "no-mixed-spaces-and-tabs": "off"
    }
}